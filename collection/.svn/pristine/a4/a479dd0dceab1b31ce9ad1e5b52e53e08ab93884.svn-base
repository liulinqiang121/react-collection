.md-right{
  margin-right: 20px;  
} 
.table {
    margin: 0 auto;
    width:100%
}
.condition-form  {
  display: block;
  float:left;
  width:660px;
  .fixed-width {
      width:250px;
      display: inline-block;
  }
  .form-btns {
      margin-right: 20px;
  }
}
