            caseCode: this.caseCode,
            caseId: this.caseId,
            caseManageId: this.caseManageId
          })
          .then(res => {
            if (res.data.code == 0) {
              this.applyderateTb = res.data.data;
            } else {
              this.$util.failCallback(res.data, this);
            }
          })
          .catch(err => {
            console.log(err);
