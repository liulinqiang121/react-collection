    padding: 0;
    width: 200px;
    //position: absolute;
    top: 0;
    //left:100px;
    margin: 0;
    display: block;
    li {
        height: 30px;
        line-height: 30px;
        margin: 0;
        display: block;
        a {
            width: 100%;
            line-height: 30px;
            height: 30px;
            padding: 0;
            margin: 0;
            text-align: left;
            i {}
        }
    }
}

// 上传结果展示
.result-upload {
    padding-left: 230px;
    font-size: 12px;
    line-height: 1;
}

.error-result {
    color: #f56c6c;
}

.success-result {
    color: #67C23A;
}

// 部门管理样式
.bd-main {
    .main-left {
        width: 50%;
        float: left;
    }
    .main-right {
        display: inline-block;
        margin-left: 50px;
    }
    .tree-wrap {
        width: 300px;
        text-align: center;
        border: 1px #ccc solid;
        height: 500px;
        overflow: auto;
    }
}

// 批量上传按钮样式 -- liulinqiang
.upload {
    width: 98px;
    text-align: center;
    position: relative;
    cursor: pointer;
    input {
        position: absolute;
        width: 98px;
        height: 40px;
        margin: -26px 0 0 -21px;
        opacity: 1;
        display: block;
        cursor: pointer;
        opacity: 0;
    }
}

// 清单下的详情
.form-detail {
    font-size: 0;
    margin-bottom: 15px;
}

.form-detail label {
    width: 80px;
    color: #99a9bf;
}

.form-detail .el-form-item {
    margin-right: 0;
    margin-bottom: 0;
    width: 20%;
    font-size: 12px;
    .el-form-item__content {
        font-size: 12px;
    }
}

// 案件详情样式 
.case-detail {
    position: relative;
    // 借款人头像
    .borrower-avatar {
        // position: absolute;
        float: left;
        // bottom: 0;
        // left: 20px;
        // display: block;
        border-radius: 50%;
    }
    // 借款人标签
    .borrower-tags {
        position: absolute;
        bottom: 0;
        left: 90px;
    }
}

// 修改外访弹窗全选的CheckBox
.out_visit_table {
    .has-gutter .cell .el-checkbox {
        display: none
    }
}

.out_visit_form {
    .el-input.is-disabled .el-input__inner,
    .el-textarea.is-disabled .el-textarea__inner {
        background: #fff;
        color: #606266
    }
}

// 催记展开
.extension .form-detail {
    margin-bottom: 0;
    label {
        width: auto;
    }
}

.qq .el-dialog__title {
    font-size: 16px;
}

// 输入框禁止写入的样式
.el-input.is-disabled .el-input__inner,
.el-textarea.is-disabled .el-textarea__inner {
    background-color: #fff;
}

// 原生文件上传样式--by liulinqiang
.a-upload {
    padding: 0px 10px;
    margin: 5px 0 0 0;
    height: 30px;
    font-size: 14px;
    line-height: 30px;
    position: relative;
    cursor: pointer;
    color: #fff;
    background: #409EFF;
    border: 1px solid #409EFF;
    border-radius: 4px;
    overflow: hidden;
    display: inline-block;
    *display: inline;
    *zoom: 1
}

.a-upload input {
    position: absolute;
    font-size: 100px;
    right: 0;
    top: 0;
    opacity: 0;
    filter: alpha(opacity=0);
    cursor: pointer
}

.a-upload:hover {
    background: #66b1ff;
    border-color: #66b1ff;
}

#fileName li {
    color: #606266;
    display: block;
    margin-right: 40px;
    overflow: hidden;
    padding-left: 4px;
    text-overflow: ellipsis;
    transition: color .3s;
    white-space: nowrap;
    min-width: 200px;
    font-size: 14px;
    height: 24px;
    line-height: 24px;
    transition: all .5s cubic-bezier(.55, 0, .1, 1);
    &:hover {}
}

// 菜单图标样式 by魏小雅
.aside-icon {
    font-size: 18px;
}

// 更多详情
.more-item .el-form-item {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    .el-form-item__content {
        width: calc( 100% - 100px);
        box-sizing: border-box;
        overflow: hidden;
        p {
            max-width: 100%;
            display: block;
            white-space: break;
            display: inline-block;
        }
    }
}

// 案件详情标签
.borrower-tags .el-badge__content {
    right: 30px;
    font-size: 10px;
}

.tabsPublic {
    padding-top: 40px;
    margin-bottom: 10px;
}

// 多级联动选择框样式调整
.el-cascader--mini .el-input__suffix {
    right: 13px;
}

.is-opened.el-cascader--mini .el-input__suffix {
    right: 5px;
}

.repayTension {
    margin: 0 0 22px 0;
    .el-form-item__label {
        width: 80px;
        display: block;
        float: left !important;
    }
    .el-form-item__content {
        display: block;
        height: auto;
    }
    .el-checkbox-group {
        margin-left: 80px;
    }
}

.personRadioBtn .el-radio-button__inner {
    text-overflow: ellipsis;
    max-width: 80px;
    overflow: hidden;
    white-space: nowrap;
    height: 34px;
    border-radius: 4px;
    padding: 9px;
    color: rgb(115, 157, 255);
    border: #DCDFE6 1px solid;
    font-size: 14px;
    margin-left: 10px;
    a {
        color: inherit;
    }
}

//外访模板样式
.template {
    .el-form-item__content {
