    }
    .note-content {
        padding-top: 5px;
        padding-left: 27px;
        word-break: break-all;
    }
    .note-icon {
        font-size: 24px;
    }
    .el-card__body{
        padding:10px
    }

}
.el-pagination {
    margin: 20px 0;
    text-align: right;
    .el-pagination__sizes {
        margin-top:-2px;
        .el-input .el-input__inner {
