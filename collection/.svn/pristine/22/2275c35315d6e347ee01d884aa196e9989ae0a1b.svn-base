// 搜索框背景颜色 -- liulinqiang
.el-input-group__append,
.el-input-group__prepend {
    background-color: #fff;
    &:hover {
        background-color: #f5f7fa
    }
}

// 搜索条件样式 --by魏小雅
.condition-form {
    .el-form-item__label {
        padding: 0 3px;
        font-size: 12px;
    }
    .fixed-width {
        width: 20%;
        float: left;
        box-sizing: border-box;
    }
    .line {
        text-align: center;
    }
    .el-date-editor--daterange.el-input__inner,
    .el-date-editor--datetimerange.el-input__inner {
        width: 100%;
    }
    .el-range-editor.el-input__inner {
        padding: 3px 0px;
    }
    .form-btns {
        float: right;
        text-align: right;
        margin-bottom: 20px;
    }
    .el-form-item--mini .el-form-item {
        margin-bottom: 0px;
    }
    .el-select .el-input {
        width: 100%;
    }
    .el-date-editor .el-range-separator {
        padding: 0;
        width: 2%;
    }
    .el-date-editor .el-range__close-icon {
        width: 10px;
        text-align: left;
    }
    .el-date-editor .el-range__icon {
        width: 12px;
        text-align: right;
        margin-left: -1px;
    }
    .el-date-editor .el-range__close-icon,
    .el-date-editor .el-range__icon {
        font-size: 12px;
    }
    .el-date-editor .el-range-input {
        width: 43%;
    }
    .el-select {
        width: 100%;
    }
}

