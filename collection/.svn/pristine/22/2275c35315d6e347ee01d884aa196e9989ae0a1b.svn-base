h5 {
    font-size: 16px
}

$client-head-bg-color:#212121;
$client-aside-and-foot-bg-color:#3D3D3D;
.content-body {
    padding: 0 10px;
    font-size:14px;
    .bd-title {
        height: 30px;
        line-height: 30px;
        border-left: solid 8px #66A3FC;
        // background-color: #66A3FC;
        // color: #fff;
        padding-left: 15px;
        font-size: 18px;
    }
    .bd-top {
        margin: 0 20px;
        padding: 10px 0;
        border-bottom: 1px solid #EEEEEE;
        h5 {
            margin-top: 8px;
            margin-bottom: 8px;
            display: inline-block;
            text-indent: 8px;
            border-left: 5px solid #7DE7F2;
            margin-right: 16px;
            vertical-align: top;
        }
    }
    .bd-main {
        // border-bottom: 2px solid #BBBBBB;
        // border-left: 2px solid #BBBBBB;
        // border-right: 2px solid #BBBBBB;
        padding: 10px 20px;
        // height: 500px;
        overflow: hidden;
        .el-table {
            width: 100%;
            border: 1px solid #EBEEF5;
        }
    }
    .bd-line {
        height: 1px;
        width: 100%;
        margin: 25px 0 10px 0;
        background: #EEEEEE;
    }
}

// 模块左右布局
.md {
    overflow: hidden;
    // margin-bottom: 20px;
    .md-left {
        float: left;
    }
    .md-right {
        float: right;
    }
}

.mb-20 {
    margin-bottom: 20px;
}

// element-ui样式修改
//翻页样式
.el-pagination {
    text-align: right;
}

.el-pagination__editor.el-input .el-input__inner {
    height: 22px;
    line-height: 22px;
}

.el-pagination span:not([class*=suffix]) {
    height: 22px;
    line-height: 22px;
    font-size: 12px;
}

.outTable .el-pagination span:not([class*=suffix]) {
    height: 22px;
    line-height: 28px;
    font-size: 12px;
}

