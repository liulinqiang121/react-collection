// 统一dialog头部样式--gaoliqing
.el-dialog {
    border-radius: 27px 27px 10px 10px;
    width: 40%!important
}

.el-dialog__body {
    width: 85%;
    margin: 0 auto;
    padding: 50px 0 30px!important;
}
.dialogDetail {
    .el-form-item__label {
        color:#99a9bf!important
    }
   
    .el-dialog__body {
        width:100%;
        padding:20px!important;
        font-size:12px;
        .el-form-item__content {
            font-size:12px;
        }
    }
}
.el-dialog__header {
    // background:#D2D3DF;
    background: rgb(234, 237, 242);
    text-align: center;
    color: #343434;
    border-radius: 27px 27px 0px 0px;
    font-size: 20px;
    line-height: 26px;
}

.el-dialog .el-form {
    width: 60%;
    margin: 0 auto;
}

.dialog_submit {
    margin-top: 40px;
    text-align: right;
}

// 遮罩-表单---gaoliqing #409EFF--组件统一的（#2BAAE1;-UI原来的，已和UI协商好）
.el-dialog .el-input__inner {
    border-left: 2px solid #409EFF;
    border-top: 1px solid #dcdfe6;
    border-bottom: 1px solid #dcdfe6;
    border-right: 1px solid #dcdfe6;
    border-radius: 2px;
}

.el-dialog .el-range-editor.el-input__inner {
    border-left: 2px solid #409EFF;
    border-top: 1px solid #dcdfe6;
    border-bottom: 1px solid #dcdfe6;
    border-right: 1px solid #dcdfe6;
    border-radius: 2px;
}

.el-dialog .el-input.is-disabled .el-input__inner,
.el-dialog .el-textarea.is-disabled .el-textarea__inner {
    border-left: 2px solid #409EFF;
    border-top: 1px solid #dcdfe6;
    border-bottom: 1px solid #dcdfe6;
    border-right: 1px solid #dcdfe6;
    border-radius: 2px;
}

