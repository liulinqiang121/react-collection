        a {
            color: #0000FF;
            text-decoration: underline
        }
    }
}

// 备忘样式 --修改间距gaoliiqng
.note-main {
    .note-time {
        float: right;
    }
    .note-title {
        font-weight: bold;
        font-size: 14px;
    }
    .note-content,
