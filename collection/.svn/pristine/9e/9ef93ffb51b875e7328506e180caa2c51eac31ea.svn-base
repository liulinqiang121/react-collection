            break;
          case '本月未跟进':
            type = 4;
            break;
          case '本月跟进':
            type = 5;
            break;
          default:
            1;
        }
        var url = (window.location.origin ? window.location.origin : '') + '/#/assignee_batch_detail?type=' + type +
