        </el-form-item>
        <el-form-item label="外访地址类型：" >
           {{outVisitData.visitAddressType}}
        </el-form-item>
        <el-form-item label="外访地址：" >
           {{outVisitData.visitAddress}}
        </el-form-item>
        <el-form-item label="新联系方式：" >
           {{outVisitData.newContact}}
        </el-form-item>
        <el-form-item label="新联系地址：" >
