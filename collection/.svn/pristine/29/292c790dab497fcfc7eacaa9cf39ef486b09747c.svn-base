<script>
  export default {
    name: 'information',
    data() {
      return {
        phoneCode: '',
        phoneInfo: {},
        qqCode: {},
        wechatCode: {},
        expressive: 'expressive',
        empty: 'empty',
        normal: 'normal',
        error: false
      };
    },
    props: {
      userInfoBean: {
        type: Object
      },
      debtInfoBeans: {
        type: Object
      },
      caseInfoBeans: {
        type: Object
      },
      moreInfo: '',
      disabledBtn: {
       
      },
      qqUniqId: {
      
      },
      wechatUniqId: {

      },
      phoneUniqId: {

      }

    },
    filters: {
      // 案件状态
      filterStatus: function (value) {
        var status;
        switch (value) {
          case '1':
            status = '新案件';
            break;
          case '2':
            status = '已还款';
            break;
          case '3':
            status = '部分还款';
            break;
          case '4':
            status = '已撤案';
            break;
          case '5':
            status = '外访中';
            break;
          case '6':
            status = '跟进中';
            break;
          default:
            status = ''
        }
        return status;
      },
      // 数据是否为空
      empty: function (value) {
        if (value === null || value === '') {
          return ' 空'
        } else {
          return value;
        }
      }
    },
    methods: {
       // 清空电话号码
      clearPhone() {
        this.phoneInfo = {uniqueId: -1};
        //console.log(this.phoneInfo)
      },

      // 打电话
      callPhone() {
       if(!this.phoneInfo.phone) {
          this.$alert("请选择呼叫对象！", "提示", {
                confirmButtonText: "确定",
                type: 'warning'
              });
              return false
        } 
        this.$emit('callPhone', this.phoneInfo)
      },

      // 发短信
      smsSend() {
        this.$emit('smsSend', this.phoneInfo)
      },

      //获取更多信息
      toMoreInfo() {
        this.$emit('toMoreInfo')
      },

      // 发送QQ
      qqSend() {
        this.$emit('qqSend',this.qqCode)
      },
      // 发送微信
      wechatSend(){
        this.$emit('wechatSend',this.wechatCode)
      },
      // 选择电话
      optionChange(item) {
        // this.phoneInfo = {}
        // debugger
        // this.phoneInfo = item;
        this.error = item.isStop ==1 ? true:false
        this.$emit('chooseConcat',item)   
      }
    },
    mounted() {
      console.log(22)
     if(this.userInfoBean.contactPhone) {
       for(var item of this.userInfoBean.contactPhone) {
       if(item.uniqueId == this.phoneUniqId) {
          this.phoneInfo = item;
          this.error = item.isStop ==1 ? true:false
       }
     }
     }
     if(this.userInfoBean.contactWechat) {
      for(var item of this.userInfoBean.contactWechat) {
       if(item.uniqueId == this.wechatUniqId) {
          this.wechatCode = item;
       }
      }
     }
     if(this.userInfoBean.contactQq) {
       for(var item of this.userInfoBean.contactQq) {
       if(item.uniqueId == this.qqUniqId) {
          this.qqCode = item;
       }
     }
     }   
    },
    created() {
      console.log(this.userInfoBean.contactPhone)
    }
  }

</script>
